{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint --type-aware --fix && eslint --fix",
    "lint:markup": "markuplint \"src/**/*.{html,jsx,tsx}\" --fix --problem-only",
    "format": "biome check --write",
    "tsc": "tsc --noEmit",
    "check": "concurrently pnpm:tsc pnpm:lint pnpm:lint:markup && pnpm format",
    "test": "concurrently pnpm:test:unit pnpm:test:storybook",
    "test:unit": "vitest --project=unit",
    "test:storybook": "vitest --project=storybook",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
