{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint --type-aware --fix && eslint --fix",
    "lint:markup": "markuplint \"src/**/*.{html,jsx,tsx}\" --fix --problem-only",
    "format": "biome check --write",
    "tsc": "tsgo --noEmit",
    "check": "concurrently pnpm:tsc pnpm:lint pnpm:lint:markup && pnpm format"
  },
  "dependencies": {
    "next": "16.0.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@markuplint/jsx-parser": "^4.7.22",
    "@markuplint/react-spec": "^4.5.22",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript/native-preview": "7.0.0-dev.20251128.1",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "^1.30.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.7.0",
    "markuplint": "^4.14.0",
    "oxlint": "^1.30.0",
    "oxlint-tsgolint": "^0.8.3",
    "typescript": "^5"
  }
}
